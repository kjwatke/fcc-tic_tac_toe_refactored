{
  "extends": ["eslint:recommended", "google"],
      "env": {
        "es6": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [2, 2],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-unused-vars": ["error", {"vars": "local"}],
        "no-undef": "off"
    }
  }